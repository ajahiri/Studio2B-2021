{
  "expo": {
    "name": "@studio2b-2021/client",
    "slug": "client",
    "version": "0.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAFAFA"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FAFAFA"
      },
      "config": {
        "googleMaps": {
          "apiKey": "################################"
        }
      },
      "package": "com.arianjahiri.authme"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
